{
  "name": "leetcode-typescript-solutions",
  "version": "1.0.0",
  "description": "LeetCode solutions implemented in TypeScript with comprehensive testing and categorized organization",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "ts-node src/index.ts"
  },
  "keywords": [
    "leetcode",
    "algorithms",
    "data-structures",
    "typescript",
    "programming",
    "interview-preparation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
